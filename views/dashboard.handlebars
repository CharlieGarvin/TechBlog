<div>
    <nav>
        <a href="/">Home</a>

        {{#if logged_in}}
            <a href="/dashboard">Dashboard</a>
            <a href="/">Logout</a>
        {{/if}}
        {{#unless logged_in}}
        <a href="/login">Dashboard</a>
        <a href="/login">Login</a>
        {{/unless}}
    </nav>

    {{#each posts}}
        <a href="/dashboard/edit/{{id}}"><h2> {{title}} </h2></a>
    <h3> {{body}} </h3>
    {{/each}}
        <a href="/dashboard/newPost"><button>Make a Post</button></a>
</div>